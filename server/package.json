{
  "name": "server",
  "description": "Multi-Agent Orchestrator System (MOS) is a real-time, multi-agent monitoring and management system that supports multiple platforms.",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "concurrently \"npm run watch:css\" \"nodemon app.js\"",
    "build:css": "npx @tailwindcss/cli -i ./public/assets/css/tailwind.css -o ./public/assets/css/styles.css --minify",
    "watch:css": "npx @tailwindcss/cli -i ./public/assets/css/tailwind.css -o ./public/assets/css/styles.css --watch"
  },
  "keywords": [
    "MOS",
    "Multi-Agent",
    "Orchestrator",
    "Monitoring",
    "Management",
    "Real-time",
    "Dashboard",
    "Agent",
    "Server",
    "IoT",
    "Desktop",
    "Mobile",
    "Multi-platform"
  ],
  "author": "Fitri HY",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "body-parser": "^2.2.0",
    "concurrently": "^9.2.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "tailwindcss": "^4.1.13",
    "wa": "^0.1.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
